{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch JS",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/${file}"
    },
    {
      "type": "node",
      "name": "JavaScript Debug Terminal",
      "request": "launch",
      "cwd": "${workspaceFolder}"
    }
  ]
}